library(Cairo)
CairoPNG(file="tmpPlots/dat2plot_1817189274.dat.png",width=1000,height=400)
n1 <- as.matrix(read.table("tmpPlots/dat2plot_1817189274.dat"))
n1 <- n1[!n1 %in% boxplot.stats(n1)$out]
avg <- mean(n1)
sd <- sd(n1)
cat("NAFlex_avg",avg,"\n")
cat("NAFlex_sd",sd,"\n")
hist(n1,breaks=100,col="lightblue",xlab="tilt",ylab="Frequency",main="Property Histogram")
abline(v=avg,col="blue",lty=2,lwd=2)
abline(v=-0.69,col="red",lty=3,lwd=2)
legend("topright", c("MD Avg","X-ray Avg"),lty=c(2,3),lwd=c(2.5,2.5),col=c("blue","red"))
dev.off()
