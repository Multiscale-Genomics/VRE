}
.social-profile ul li.you-tube a {
  background: url(images/youtube.png) no-repeat 0 0;
}
.social-profile ul li.you-tube a:hover {
  background: url(images/youtube.png) no-repeat 0 0;
}
.social-profile ul li.google-plus a {
